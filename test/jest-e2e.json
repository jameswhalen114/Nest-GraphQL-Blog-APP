{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/current-user/(.*)": "<rootDir>/../libs/current-user/src/$1",
    "@app/current-user": "<rootDir>/../libs/current-user/src"
  }
}